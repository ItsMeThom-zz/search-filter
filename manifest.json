{
	"manifest_version": 2,
	"name": "Filter",
	"version": "1.0",
	"description": "filter domains from duckduckgo results",
	"content_scripts": [
		{
			"matches": [
				"*://*.duckduckgo.com/*"
			],
			"js": [
				"filter.js"
			]
		}
	],
	"permissions": [
		"*://*.duckduckgo.com/*",
		"storage"
	]
}